<Burger key={el} bg={\"rgba(0,0,0,0.2)\"} />;\n            })}\n          </Burgers>\n        </Menu>\n      </Wrapper>\n    );\n  }\n}\n","/Users/rostyk/Desktop/macscreen 3/front/src/redux/reducer/burgersBucket.js",["211"],"import produce from \"immer\";\n\nconst initialState = {\n  isLoading: true,\n  error: \"\",\n  burgers: [],\n};\n\nexport default function (state = initialState, action) {\n  switch (action.type) {\n    case \"ADD_BURGER_BUCKET\": {\n      return produce(state, (draftState) => {\n        if (Array.isArray(action.payload)) {\n          draftState.burgers.push(...action.payload);\n        } else {\n          draftState.burgers.push(action.payload);\n        }\n      });\n    }\n    case \"REMOVE_BURGERS_BUCKET\": {\n      return produce(state, (draftState) => {\n        draftState.burgers = []\n      });\n    }\n    case \"REMOVE_ONE_BURGER_BUCKET\": {\n      return produce(state, (draftState) => {\n        draftState.burgers = draftState.burgers.filter(el => el.id !== action.payload)\n      })\n    }\n    default:\n      return state;\n  }\n}\n","/Users/rostyk/Desktop/macscreen 3/front/src/redux/reducer/Burger.js",["212","213"],"/Users/rostyk/Desktop/macscreen 3/front/src/pages/home/homestyled.js",[],"/Users/rostyk/Desktop/macscreen 3/front/src/pages/redo/burgerredostyled.js",[],"/Users/rostyk/Desktop/macscreen 3/front/src/helpers/sum.js",[],"/Users/rostyk/Desktop/macscreen 3/front/src/hooks/useBurger.js",["214"],"/Users/rostyk/Desktop/macscreen 3/front/src/component/burger/index.js",[],"/Users/rostyk/Desktop/macscreen 3/front/src/component/IngridientsDetailBlock/index.js",["215"],"/Users/rostyk/Desktop/macscreen 3/front/src/component/Ingridient/index.js",["216","217"],"/Users/rostyk/Desktop/macscreen 3/front/src/component/IngridientsDetailBlock/ingridientsdetailblockstyled.js",[],"/Users/rostyk/Desktop/macscreen 3/front/src/component/burger/burgerstyled.js",[],"/Users/rostyk/Desktop/macscreen 3/front/src/component/Ingridient/ingridientstyled.js